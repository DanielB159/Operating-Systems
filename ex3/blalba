# 	gcc -o ex3.out main.o BoundedQueue.o Producer.o

# main.o: main.c BoundedQueue.h Producer.h
# 	gcc -c -pthread -o main.o main.c

# BoundedQueue.o: 
# 	gcc -c -o BoundedQueue.o BoundedQueue.c

# Producer.o: Producer.c Producer.h BoundedQueue.h
# 	gcc -c -o Producer.o Producer.c

# clean:
# 	rm -f *.out *.o *.gch